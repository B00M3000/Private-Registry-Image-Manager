{
  "name": "private-registry-image-manager",
  "version": "1.2.0",
  "description": "A image manager CLI tool for private registries. Tailored toward internal registry management for rapid deployment of services.",
  "main": "dist/cli.js",
  "bin": {
  "private-registry-image-manager": "dist/cli.js",
  "container-deploy": "dist/cli.js",
  "prim": "dist/cli.js"
  },
  "scripts": {
  "build": "tsc",
  "dev": "ts-node src/cli.ts",
  "start": "node dist/cli.js",
  "test": "jest --runInBand",
  "test:watch": "jest --watch",
  "lint": "eslint src --ext .ts",
  "lint:fix": "eslint src --ext .ts --fix",
  "prepare": "pnpm run build",
  "prepublishOnly": "pnpm run build && pnpm test"
  },
  "keywords": [
    "docker",
    "deployment",
    "registry",
    "container",
    "cli",
    "devops"
  ],
  "author": "Thomas Zhou <thomaszhou07@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/B00M3000/Private-Registry-Image-Manager.git"
  },
  "packageManager": "pnpm@9.0.0",
  "files": [
    "dist/**/*",
    "templates/**/*",
    "README.md",
    "LICENSE"
  ],
  "engines": {
  "node": ">=18.0.0",
  "pnpm": ">=8"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "inquirer": "^8.2.6",
    "js-yaml": "^4.1.0",
    "ora": "^5.4.1",
    "simple-git": "^3.20.0",
    "which": "^4.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.9.0",
    "@types/which": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
